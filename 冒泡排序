#include(stdio.h)
int main()
{
  int i,j;
  int k;
  int a[5];
  int temp;
  for(k=1;k<=5;k++)
  {
    printf("输入数据：")；
    scanf("%d",a[k]);
  }
  for(i=1;i<=4;i++)
  {
    for(j=1;j<5-i;j++)
    {
      if(a[j]>a[j+1])
      {
        temp=a[j];
        a[j]=a[j+1];
        a[j+1]=temp;
      }
    }
  }
  printf("\n");
  for(i=1;i<=5;i++)
  {
    printf("%d",a[i]);
  }
  return o;
}
